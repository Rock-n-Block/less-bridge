\";\n  const isNetworkBinanceSmartChain = network === \"Binance-Smart-Chain\";\n  const isNetworkEthereum = network === \"Ethereum\";\n  if (network === 'Matic') {\n    if (config.IS_PRODUCTION) {\n      return `${config.tokenLinks().matic}${tokenAddress}/read-contract`\n    }\n    return `${config.tokenLinks().matic}${tokenAddress}/contracts`\n  }\n  return isNetworkBinanceChain\n    ? \"https://explorer.binance.org/asset/WISH-2D5\"\n    : config.tokenLinks()[\n    isNetworkEthereum\n      ? \"ethereum\"\n      : isNetworkBinanceChain\n        ? \"binanceChain\"\n        : \"binanceSmartChain\"\n    ] + `/token/${tokenAddress}`;\n};\n\nexport const getTokenSymbol = (network) => {\n  return network === \"Binance-Chain\"\n    ? \"LESS\"\n    : network === \"Binance-Smart-Chain\"\n      ? \"BSC LESS\"\n      : \"LESS\";\n};\n","/Users/gleb/projects/less-bridge/src/redux/actions/modal.js",["212"],"export default {\n    toggleModal: data => ({\n        type: 'MODAL:TOGGLE',\n        payload: data\n    })\n}","/Users/gleb/projects/less-bridge/src/redux/actions/user.js",["213"],"export default {\n    setUserData: data => ({\n        type: 'USER:SET_DATA',\n        payload: data\n    })\n}","/Users/gleb/projects/less-bridge/src/redux/actions/wallet.js",["214"],"export default {\n  setWalletType: data => ({ type: 'WALLET:SET_TYPE', payload: data }),\n  setWalletNetFrom: data => ({ type: 'WALLET:SET_NETWORK_FROM', payload: data }),\n  setWalletNetTo: data => ({ type: 'WALLET:SET_NETWORK_TO', payload: data }),\n  setWalletDexList: data => ({ type: 'WALLET:SET_DEX_LIST', payload: data }),\n  setWalletDex: data => ({ type: 'WALLET:SET_DEX', payload: data }),\n}\n","/Users/gleb/projects/less-bridge/src/redux/actions/form.js",["215"],"export default {\n    showFormError: data => ({\n        type: 'FORM:ERROR',\n        payload: data\n    })\n}\n","/Users/gleb/projects/less-bridge/src/utils/backend/index.js",[],"/Users/gleb/projects/less-bridge/src/utils/metamask/index.js",[],"/Users/gleb/projects/less-bridge/src/utils/binance/index.js",[],"/Users/gleb/projects/less-bridge/src/utils/contractService/index.js",["216"],"import React from 'react';\nimport BigNumber from 'bignumber.js';\nimport config from '../../config';\n\nexport default class ContractService {\n  constructor({ wallet, networkFrom, contractDetails }) {\n    console.log('ContractService:', networkFrom, contractDetails);\n    this.wallet = wallet;\n    this.net = config.IS_PRODUCTION ? 'mainnet' : 'testnet';\n    this.contractAddressToken = contractDetails.ADDRESS.TOKEN[networkFrom];\n    this.contractAddressSwap = contractDetails.ADDRESS.SWAP[networkFrom];\n    this.contractAbiToken = contractDetails.ABI.TOKEN[networkFrom];\n    this.contractAbiSwap = contractDetails.ABI.SWAP[networkFrom];\n    this.decimals = contractDetails.DECIMALS.TOKEN[networkFrom];\n    if (networkFrom === 'Binance-Chain') return;\n    this.contractToken = this.wallet.getContract(\n      this.contractAbiToken,\n      this.contractAddressToken,\n    );\n    this.contractSwap = this.wallet.getContract(\n      this.contractAbiSwap,\n      this.contractAddressSwap,\n    );\n  }\n\n  balanceOf = async (address) => {\n    // console.log('balanceOf',this.contractAddressToken,address,this.contractToken)\n    try {\n      const balance = await this.contractToken.methods\n        .balanceOf(address)\n        .call();\n      return +new BigNumber(balance._hex ? parseInt(balance._hex) : balance)\n        .dividedBy(new BigNumber(10).pow(this.decimals))\n        .toFixed();\n    } catch (error) {\n      console.log(error, 'get balance');\n    }\n  };\n\n  allowance = async (address) => {\n    // console.log('allowance',address,this.contractAddressSwap)\n    const allowance = await this.contractToken.methods\n      .allowance(address, this.contractAddressSwap)\n      .call();\n    return +new BigNumber(allowance)\n      .dividedBy(new BigNumber(10).pow(this.decimals))\n      .toString(10);\n  };\n\n  totalSupply = async () => {\n    const totalSupply = await this.contractToken.methods.totalSupply().call();\n    return +new BigNumber(totalSupply)\n      .dividedBy(new BigNumber(10).pow(this.decimals))\n      .toString(10);\n  };\n\n  approveToken = async (address, callback) => {\n    const totalSupply = await this.totalSupply();\n    this.wallet.approveToken(\n      address,\n      this.contractAddressSwap,\n      totalSupply,\n      callback,\n    );\n  };\n\n  transferFromBinanceChain = async ({\n    userAddress,\n    blockchain,\n    amount,\n    receiver,\n  }) => {\n    try {\n      const data = `${blockchain}${receiver}`;\n      return await this.wallet.sendPlainTx({\n        addressFrom: userAddress,\n        amount,\n        data,\n      });\n    } catch (e) {\n      console.error(e);\n    }\n  };\n\n  transferToOtherBlockchain = async ({\n    userAddress,\n    blockchain,\n    amount,\n    receiver,\n    callback,\n  }) => {\n    this.wallet.transferToOtherBlockchain({\n      userAddress,\n      blockchain,\n      amount,\n      receiver,\n      callback,\n    });\n  };\n}\n","/Users/gleb/projects/less-bridge/src/utils/matic/index.js",[],"/Users/gleb/projects/less-bridge/src/utils/tron/index.js",["217"],{"ruleId":"218","replacedBy":"219"},{"ruleId":"220","replacedBy":"221"},{"ruleId":"222","severity":1,"message":"223","line":14,"column":9,"nodeType":"224","messageId":"225","endLine":14,"endColumn":24},{"ruleId":"226","severity":1,"message":"227","line":1,"column":1,"nodeType":"228","endLine":69,"endColumn":3},{"ruleId":"226","severity":1,"message":"229","line":8,"column":1,"nodeType":"228","endLine":19,"endColumn":2},{"ruleId":"222","severity":1,"message":"230","line":1,"column":25,"nodeType":"224","messageId":"225","endLine":1,"endColumn":37},{"ruleId":"226","severity":1,"message":"229","line":14,"column":1,"nodeType":"228","endLine":23,"endColumn":2},{"ruleId":"226","severity":1,"message":"229","line":7,"column":1,"nodeType":"228","endLine":17,"endColumn":2},{"ruleId":"2